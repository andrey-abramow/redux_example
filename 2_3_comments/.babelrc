{
  "env": {
    "development": {
       presets: ['es2015', 'react'],
       plugins: [
         ['transform-object-rest-spread'],
         ['react-transform', {
           transforms: [
             {
               transform: 'react-transform-hmr',
               imports: ['react'],
               locals: ['module']
             }
           ],
         }],
       ],
    },
     "test": {
         presets: ['es2015', 'react'],
         plugins: [
           ['transform-object-rest-spread'],
         ],
      },
  }
}
